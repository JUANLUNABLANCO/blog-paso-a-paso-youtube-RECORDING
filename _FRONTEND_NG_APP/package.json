{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng:build:prod": "rimraf ./dist && ng build --configuration=prod",
    "ng:build:dev": "ng build --configuration=dev",
    "ng:serve:dev": "ng serve  --open",
    "ng:serve:local": "ng serve --configuration=local --open",
    "ng:serve:test": "ng serve --configuration=test --open",
    "ng:serve:prod": "ng serve  --configuration=prod",
    "ng:serve:prod-local": "npx http-server -p 8080 --cors -c-1 dist",
    "ng:build-serve:prod-local": "rimraf ./dist && ng build --configuration=production && npx http-server -p 8080 -c-1 dist",
    "ng:test": "ng test",
    "ng:test:outbrowser": "ng test --browsers=ChromeHeadless",
    "ng:test:ci": "ng test --no-watch --code-coverage",
    "ng:lint": "ng lint",
    "ng:lint:fix": "ng lint --fix",
    "ng:format": "prettier --write \"src/**/*.ts\" \"src/**/*.html\"",
    "ng:format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.html\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.9",
    "@angular/cdk": "^16.1.4",
    "@angular/common": "^16.2.9",
    "@angular/compiler": "^16.2.9",
    "@angular/core": "^16.2.9",
    "@angular/forms": "^16.2.9",
    "@angular/material": "^16.1.4",
    "@angular/platform-browser": "^16.2.9",
    "@angular/platform-browser-dynamic": "^16.2.9",
    "@angular/router": "^16.2.9",
    "@angular/service-worker": "^16.1.0",
    "@auth0/angular-jwt": "^5.1.2",
    "marked": "^12.0.2",
    "ngx-markdown": "^17.2.1",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "~16.2.6",
    "@angular/compiler-cli": "^16.2.9",
    "@faker-js/faker": "^8.0.2",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "cypress": "^12.17.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "pg": "^8.11.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "typescript": "~5.1.3"
  }
}
